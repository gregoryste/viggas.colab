<div class="which-already-built">
    <div class="container">
        <h1 class="which-already-built__title">Com quem já construímos?</h1>
        <div class="which-already-built__container">
            <div class="which-already-built__text">
                {{ partnerText }}
            </div>
                <div class="swiper js-carousel-partners">
                    <div class="swiper-wrapper">
                        {% for partner in partners %}
                            <div class="swiper-slide">
                                {% if partner.link and partner.link != "" %}
                                    <a href="{{ partner.link }}">
                                        <img src="{{ partner.image }}" alt="Parceiro" width="100%" height="100%" loading="lazy"> 
                                    </a>
                                {% else %}
                                    <img src="{{ partner.image }}" alt="Parceiro" width="100%" height="100%" loading="lazy">
                                {% endif  %}
                            </div>
                        {% endfor %}
                    </div>
                    <div class="default-style__arrow js-button-next">
                        <img src="/images/icon-arrow-r-partners.webp" class="which-already-built__image" alt="Seta Proximo" width="100%" height="100%" loading="lazy">
                    </div>
                    <div class="default-style__arrow js-button-prev">
                        <img src="/images/icon-arrow-l-partners.webp" class="which-already-built__image" alt="Seta Anterior" width="100%" height="100%" loading="lazy">
                    </div>
                </div>
        </div>
        <div class="which-already-built__container-button">
            <button type="button" class="which-already-built__button">Veja mais sobre o nossos Casos</button>
        </div>
    </div>
</div>